<%= simple_form_for(@review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :positive %>
    <%= f.input :negative %>
    <%= f.input :comments %>
    <%= f.input :user_id %>
    <%= f.input :post_id %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
